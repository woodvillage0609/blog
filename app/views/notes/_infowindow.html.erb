<div>

<li><%=link_to image_tag(picture,{class:"infowindow-photo"}), note_path(id) %></li>

<li class="star-rating" data-score=<%= rating %>></li>

</div>

<script>
    $('.star-rating').raty({
      path: '/assets/',
      readOnly: true,
      score: function() {
            return $(this).attr('data-score');
    }
  });
</script>